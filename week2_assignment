void main() {
  // Create an empty list
  List<int> myList = [];

  // Append elements to myList
  myList.add(10);
  myList.add(20);
  myList.add(30);
  myList.add(40);
  
  // Insert the value 15 at the second position
  myList.insert(1, 15);  // Index 1 is the second position
  
  // Extend myList with another list
  myList.addAll([50, 60, 70]);
  
  // Remove the last element from myList
  myList.removeLast();
  
  // Sort myList in ascending order
  myList.sort();
  
  // Find and print the index of the value 30
  int indexOf30 = myList.indexOf(30);
  print('The index of 30 is: $indexOf30');
  
  // Print the final list
  print('Final sorted list: $myList');
}
